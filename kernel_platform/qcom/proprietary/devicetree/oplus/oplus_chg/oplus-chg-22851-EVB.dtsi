/*#ifdef OPLUS_FEATURE_CHG_BASIC*/
&tlmm {
	oplus_ufcs_gpio: oplus_ufcs_gpio {
		ufcs_en_active: ufcs_en_active {
			mux {
				pins = "gpio66";
				function = "gpio";
			};

			config {
				pins = "gpio66";
				drive-strength = <2>;
				bias-disable;
				output-high;
			};
		};
		ufcs_en_sleep: ufcs_en_sleep {
			mux {
				pins = "gpio66";
				function = "gpio";
			};

			config {
				pins = "gpio66";
				drive-strength = <2>;
				bias-disable;
				output-low;
			};
		};

		ufcs_int_default: ufcs_int_default {
			mux {
				pins = "gpio18";
				function = "gpio";
			};

			config {
				pins = "gpio18";
				drive-strength = <2>;
				bias-pull-up;
				input-enable;
			};
		};
	};
};

