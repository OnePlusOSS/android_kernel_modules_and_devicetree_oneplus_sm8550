&tlmm {
	oplus_wls_gpio: oplus_wls_gpio {
		cp_int_default: cp_int_default {
			mux {
				pins = "gpio85";
				function = "gpio";
			};

			config {
				pins = "gpio85";
				drive-strength = <2>;
				bias-disable;
				input-enable;
			};
		};

		rx_event_default: rx_event_default {
			mux {
				pins = "gpio9";
				function = "gpio";
			};

			config {
				pins = "gpio9";
				drive-strength = <2>;
				input-enable;
				bias-disable;
			};
		};

		rx_mode_default: rx_mode_default {
			mux {
				pins = "gpio63";
				function = "gpio";
			};

			config {
				pins = "gpio63";
				drive-strength = <2>;
				bias-disable;
				output-low;
			};
		};

		rx_mode_active: rx_mode_active {
			mux {
				pins = "gpio63";
				function = "gpio";
			};

			config {
