&qupv3_se12_i2c {
	status = "ok";
	qcom,clk-freq-out = <400000>;

	oplus_hl7227: oplus,hl7227@51 {
		status = "ok";
		compatible = "oplus,hl7227-cp";
		reg = <0x51>;
		oplus,ic_type = <OPLUS_CHG_IC_CP_DIV2>;
		oplus,ic_index = <0>;

		oplus,cp_en-gpio = <&pm8550ve_f_gpios 8 0x00>;
		oplus,cp_int-gpio = <&tlmm 85 0x00>;
		pinctrl-names = "cp_en_active", "cp_en_sleep", "cp_int_default";
		pinctrl-0 = <&cp_en_active>;
		pinctrl-1 = <&cp_en_sleep>;
		pinctrl-2 = <&cp_int_default>;
	};

	oplus_nu1619: oplus,nu1619@61 {
		status = "ok";
		compatible = "oplus,nu1619-rx";
		reg = <0x61>;
		#address-cells = <1>;
		#size-cells = <0>;
		oplus,ic_type = <OPLUS_CHG_IC_RX>;
		oplus,ic_index = <0>;
		oplus,support_epp_11w;

		oplus,rx_event-gpio = <&tlmm 9 0x00>;//check
		oplus,rx_con-gpio = <&tlmm 8 0x00>;//check
		oplus,rx_en-gpio = <&pm8550vs_c_gpios 6 0x00>;//vt-sleep check
		oplus,rx_mode-gpio = <&tlmm 63 0x00>;//check
		oplus,dcdc_en-gpio = <&tlmm 62 0x00>;
		pinctrl-names = "rx_event_default", "rx_con_default",
				"rx_en_sleep", "rx_en_active",
				"rx_mode_default", "rx_mode_active",
				"rx_mode_sleep",
				"dcdc_en_active", "dcdc_en_sleep";
		pinctrl-0 = <&rx_event_default>;
		pinctrl-1 = <&rx_con_default>;
		pinctrl-2 = <&rx_en_sleep>;
		pinctrl-3 = <&rx_en_active>;
		pinctrl-4 = <&rx_mode_default>;
		pinctrl-5 = <&rx_mode_active>;
		pinctrl-6 = <&rx_mode_sleep>;
		pinctrl-7 = <&dcdc_en_active>;
		pinctrl-8 = <&dcdc_en_sleep>;
	};
};

