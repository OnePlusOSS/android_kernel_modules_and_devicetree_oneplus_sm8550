&pm8550b_eusb2_repeater {
	qcom,param-override-seq = <0x0e 0x51 0x06 0x53 0x06 0x54 0x06 0x57>;
	qcom,param-override-seq-host = <0x0e 0x51 0x06 0x53 0x06 0x54 0x06 0x57>;
};

&regulator_ocp_notifier {
	periph-1cb-supply = <0>;
	periph-1cc-supply = <0>;
	periph-1cd-supply = <0>;
	periph-4c1-supply = <0>;
	periph-4c3-supply = <0>;
};

&pm8550b_haptics {
	qcom,vmax-mv = <1680>;
	qcom,cl-vmax-mv = <1680>;
	qcom,lra-period-us = <5882>;
	qcom,fifo-vmax-mv = <8500>;
	qcom,old-steady-vmax-mv = <8500>;
	qcom,brake-mode = <BRAKE_OPEN_LOOP>;
	qcom,cal-data-restore;
	qcom,fifo-empty-threshold = <480>;
	qcom,vibrator-type = <8151>;//sla0815
	oplus,livetap_support;
};

