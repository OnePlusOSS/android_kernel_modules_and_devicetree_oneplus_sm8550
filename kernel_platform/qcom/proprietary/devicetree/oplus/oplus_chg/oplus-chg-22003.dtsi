&soc {
	oplus,track-charge {
		compatible = "oplus,track-charge";
		track,fast_chg_break_t_thd = <1000>;
		track,general_chg_break_t_thd = <200>;
		track,voocphy_type = <3>;
		track,wired_max_power = <67000>;
		track,wired_fast_chg_scheme = <3>;
	};
};

&usb0 {
	dwc3@a600000 {
		compatible = "snps,dwc3";
		maximum-speed = "super-speed";
		dr_mode = "otg";
	};
};

&qupv3_se8_i2c_sda_active {
	mux {
		pins = "gpio0";
		function = "qup2_se0_l0_mirb";
	};

	config {
		pins = "gpio0";
		drive-strength = <2>;
		bias-pull-up;
	};
};

&qupv3_se8_i2c_scl_active {
	mux {
		pins = "gpio1";
		function = "qup2_se0_l1_mirb";
	};

	config {
		pins = "gpio1";
		drive-strength = <2>;
		bias-pull-up;
	};
};

&qupv3_se8_i2c_sleep {
	mux {
		pins = "gpio0", "gpio1";
		function = "gpio";
	};

	config {
		pins = "gpio0", "gpio1";
		drive-strength = <2>;
		bias-disable;
	};
};

&qupv3_se8_i2c {
	status = "ok";
	qcom,clk-freq-out = <100000>;
	bq27541-battery@55 {
		status = "okay";
		compatible = "oplus,bq27541-battery";
		reg = <0x55>;
		qcom,modify-soc-smooth;
		oplus,capacity-pct = <60>;
	};

	sc8547-charger-slave@6F{
		status = "okay";
		compatible = "sc,sc8547-slave";
		reg = <0x6F 0>;
		ocp_reg = <12>;
	};
};

&qupv3_hub_i2c3 {
	status = "ok";
	qcom,clk-freq-out = <100000>;

	bq27541-battery@55 {
		status = "okay";
		compatible = "oplus,bq27541-battery";
		reg = <0x55>;
		qcom,modify-soc-smooth;
		qcom,gauge_num = <1>;
		oplus,capacity-pct = <40>;
	};
};

